<table class="ui table">
  <thead>
    <tr>
      <th>Song</th>
      <th>Artist</th>
      <th>Delete</th>
      
    </tr>    
  </thead>
  <tbody>
    {{#each playlist.songs}}   
      <tr>
        <tr>
        <td>
          {{title}}
        </td>
        <td>
          {{artist}}
        </td>
        <td>
          <a href="/playlist/{{../playlist.id}}/deletesong/{{id}}" class="ui tiny red button">Delete Song</a>
        </td>
      </tr> 
    </tr> 
    
    {{/each}}
    
  </tbody>
</table>